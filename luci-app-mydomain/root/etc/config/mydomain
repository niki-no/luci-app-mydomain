config ddns 'example'
    option enabled '0'
    option domain 'example.com'
    option provider 'dnspod'
    option api_key 'your_api_key'
    option secret 'your_secret'
    option ipv4 '1'
    option ipv6 '0'
    option interval '30'
    option record_type 'A'
    option subdomain '@'

config proxy 'web_proxy'
    option enabled '0'
    option name 'Web Proxy'
    option frontend_port '8080'
    option backend_url '127.0.0.1:80'
    option protocol 'http'
    option lb_algorithm 'roundrobin'
    option health_check '1'
    option health_check_url '/health'

config certificate 'example_cert'
    option enabled '0'
    option domain 'example.com'
    option email 'admin@example.com'
    option validation 'http'
    option cert_path '/etc/ssl/certs/example.com.crt'
    option key_path '/etc/ssl/private/example.com.key'
    option renew_interval '60'
